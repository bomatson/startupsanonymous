%h4 Please select a time you are available 
= form_for @timeslot, html: {class: 'custom'} do |f|
  .row
    .large-6.columns
      %h2 Start Time
      %div.datetime
        = f.datetime_select(:start_time, default: DateTime.now, datetime_separator: "", time_separator: "", ampm: true, :order => [:day, :month, :year, :hour, :minute], :minute_step => 15, html_options: {class: 'timeslot-date'})
  .row
    .large-6.columns
      %h2 End Time
      %div.datetime
        = f.datetime_select(:end_time, default: DateTime.now + 1.hour, ampm: true, datetime_separator: "", time_separator: "", :order => [:day, :month, :year, :hour, :minute], :minute_step => 15, html_options: {class: 'timeslot-date'})
  .row
    .large-4.columns
      = f.submit :submit, class: 'button prefix', id: 'timeslot'